name = Event Workflow
description = This extension module integrates with the ImageX [Workflow](http://github.com/imagex/imagex_workflow) component to provide a default publishing and scheduling workflow for Event content.
core = 7.x
package = ImageX
version = 7.x-1.0
project = imagex_events_workflow
dependencies[] = ctools
dependencies[] = imagex_events
dependencies[] = strongarm
features[ctools][] = strongarm:strongarm:1
features[features_api][] = api:2
features[variable][] = scheduler_publish_enable_event
features[variable][] = scheduler_publish_moderation_state_event
features[variable][] = scheduler_publish_required_event
features[variable][] = scheduler_publish_revision_event
features[variable][] = scheduler_publish_touch_event
features[variable][] = scheduler_unpublish_enable_event
features[variable][] = scheduler_unpublish_required_event
features[variable][] = scheduler_unpublish_revision_event
features[variable][] = workbench_access_node_type_event
features[variable][] = workbench_moderation_default_state_event
